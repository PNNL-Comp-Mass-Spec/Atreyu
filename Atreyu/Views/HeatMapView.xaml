<UserControl x:Class="Falkor.Views.Atreyu.HeatMapView"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
			 xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
			 xmlns:oxy="http://oxyplot.codeplex.com"
			 xmlns:wpf="http://oxyplot.org/wpf"
			 mc:Ignorable="d" 
			 d:DesignHeight="300" d:DesignWidth="300"
			 AllowDrop="True">
	<Grid>
		<wpf:PlotView x:Name="HeatMapPlot" Model="{Binding HeatMapPlotModel}" Height="auto" Background="{x:Null}" Margin="0,31,0,0" />
		<TextBlock TextWrapping="Wrap" Text="Heatmap" Margin="0,0,0,269" HorizontalAlignment="Center"/>
	</Grid>
</UserControl>
