<Window x:Class="Viewer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:y="clr-namespace:Atreyu.Views;assembly=Atreyu"
        Title="MainWindow" Height="600" Width="800">
    <DockPanel Name="MainDockPanel">
        <ToolBarTray DockPanel.Dock="Top">
            <ToolBar>
                <Button Name="OpenButton" Content="Open" />
                <Button Name="SaveButton" Content="Save Heatmap (WiP)"/>
            </ToolBar>
        </ToolBarTray>
        <y:CombinedHeatmapView Name="CombinedHeatmapView"/>
    </DockPanel>
</Window>
